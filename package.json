{
    "name": "capacitor-oauth2",
    "version": "1.0.2",
    "description": "Simple Capacitor OAuth 2 client plugin",
    "author": "Michael Oberwasserlechner forked by Carlos Mart√≠nez",
    "homepage": "https://github.com/carlosmart7104/capacitor-oauth2",
    "license": "MIT",
    "main": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "scripts": {
        "build": "npm run clean && tsc",
        "clean": "rm -rf ./dist",
        "watch": "tsc --watch",
        "test": "jest",
        "removePacked": "rm -f byteowls-capacitor-oauth2-*.tgz",
        "publishLocally": "npm run removePacked && npm run build && npm pack",
        "prepublishOnly": "npm run build"
    },
    "files": [
        "dist/",
        "ios/",
        "android/",
        "ByteowlsCapacitorOauth2.podspec"
    ],
    "keywords": [
        "capacitor",
        "capacitor-plugin",
        "oauth2",
        "oauth2-client",
        "social-login"
    ],
    "capacitor": {
        "ios": {
            "src": "ios"
        },
        "android": {
            "src": "android"
        }
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/carlosmart7104/capacitor-oauth2"
    },
    "bugs": {
        "url": "https://github.com/carlosmart7104/capacitor-oauth2/issues"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://npm.pkg.github.com/@carlosmart7104"
    },
    "peerDependencies": {
        "@capacitor/core": "^2.0.0"
    },
    "dependencies": {},
    "devDependencies": {
        "@capacitor/core": "2.0.0",
        "@capacitor/ios": "2.0.0",
        "@capacitor/android": "2.0.0",
        "typescript": "^3.5.3",
        "@types/jest": "25.2.1",
        "jest": "25.2.7",
        "ts-jest": "25.3.1"
    }
}
